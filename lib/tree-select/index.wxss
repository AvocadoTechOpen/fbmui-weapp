@import '../common/index.wxss';.fbm-tree-select{display:flex;font-size:var(--tree-select-font-size,14px);position:relative;-webkit-user-select:none;user-select:none}.fbm-tree-select__nav{--sidebar-padding:12px 8px 12px 12px;background-color:var(--tree-select-nav-background-color,#f7f8fa);flex:1}.fbm-tree-select__nav__inner{height:100%;width:100%!important}.fbm-tree-select__content{background-color:var(--tree-select-content-background-color,#fff);flex:2}.fbm-tree-select__item{font-weight:700;line-height:var(--tree-select-item-height,44px);padding:0 32px 0 var(--padding-md,16px);position:relative}.fbm-tree-select__item--active{color:var(--tree-select-item-active-color,#ee0a24)}.fbm-tree-select__item--disabled{color:var(--tree-select-item-disabled-color,#c8c9cc)}.fbm-tree-select__selected{position:absolute;right:var(--padding-md,16px);top:50%;transform:translateY(-50%)}