@import '../common/index.wxss';.fbm-dialog{background-color:var(--dialog-background-color,#fff);border-radius:var(--dialog-border-radius,16px);font-size:var(--dialog-font-size,16px);overflow:hidden;top:45%!important;width:var(--dialog-width,320px)}@media (max-width:321px){.fbm-dialog{width:var(--dialog-small-screen-width,90%)}}.fbm-dialog__header{font-weight:var(--dialog-header-font-weight,500);line-height:var(--dialog-header-line-height,24px);padding-top:var(--dialog-header-padding-top,24px);text-align:center}.fbm-dialog__header--isolated{padding:var(--dialog-header-isolated-padding,24px 0)}.fbm-dialog__message{-webkit-overflow-scrolling:touch;font-size:var(--dialog-message-font-size,14px);line-height:var(--dialog-message-line-height,20px);max-height:var(--dialog-message-max-height,60vh);overflow-y:auto;padding:var(--dialog-message-padding,24px);text-align:center}.fbm-dialog__message-text{word-wrap:break-word}.fbm-dialog__message--hasTitle{color:var(--dialog-has-title-message-text-color,#646566);padding-top:var(--dialog-has-title-message-padding-top,8px)}.fbm-dialog__message--round-button{color:#323233;padding-bottom:16px}.fbm-dialog__message--left{text-align:left}.fbm-dialog__message--right{text-align:right}.fbm-dialog__message--justify{text-align:justify}.fbm-dialog__footer{display:flex}.fbm-dialog__footer--round-button{padding:8px 24px 16px!important;position:relative!important}.fbm-dialog__button{flex:1}.fbm-dialog__cancel,.fbm-dialog__confirm{border:0!important}.fbm-dialog-bounce-enter{opacity:0;transform:translate3d(-50%,-50%,0) scale(.7)}.fbm-dialog-bounce-leave-active{opacity:0;transform:translate3d(-50%,-50%,0) scale(.9)}